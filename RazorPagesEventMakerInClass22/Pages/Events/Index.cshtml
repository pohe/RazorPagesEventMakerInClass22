@page
@model RazorPagesEventMakerInClass22.Pages.Events.IndexModel
@{
    ViewData["Title"] = "Event oversigt";
}

<h1>LIst of events</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Description</th>
            <th>Place</th>
            <th>DateTime</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < 5; i++)
        {
            <tr>
                <td>@i id</td>
                <td>@i Name</td>
                <td>@i Description</td>
                <td>@i Place</td>
                <td>@i Datetime</td>
            </tr>

        }

    </tbody>
</table>
